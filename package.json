{
  "name": "is-retiro-open",
  "version": "1.0.7",
  "description": "¿Está abierto el Retiro? Real-time Retiro Park status checker",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build:client": "tsc && vite build --outDir dist",
    "build:server": "vite build --ssr src/entry-server.tsx --outDir dist/server",
    "build": "npm run build:client && npm run build:server && tsx prerender.ts",
    "test": "tsx --test api/**/*.test.ts src/**/*.test.ts",
    "preview": "vite preview",
    "generate-og": "tsx scripts/generate-og-images.ts"
  },
  "keywords": [
    "retiro",
    "madrid",
    "park",
    "status"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.90.20",
    "@vercel/analytics": "^1.6.1",
    "@vercel/speed-insights": "^1.3.1",
    "lucide-react": "^0.563.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "@vercel/node": "^5.5.28",
    "@vitejs/plugin-react": "^5.1.2",
    "canvas": "^3.2.1",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1"
  }
}
